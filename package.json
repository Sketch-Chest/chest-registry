{
  "name": "chest-registry",
  "version": "1.0.0",
  "description": "Chest registry service.",
  "private": true,
  "main": "app.js",
  "scripts": {
    "postinstall": "npm --prefix client install",
    "start": "node ./bin/www",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "run-p test:*",
    "test:server": "nyc ava && xo",
    "test:client": "npm --prefix client test",
    "watch": "run-p watch:*",
    "watch:server": "nodemon ./bin/www",
    "watch:client": "npm --prefix client start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sketch-Chest/chest-registry.git"
  },
  "keywords": [],
  "author": "Yasuaki Uechi <uetchy@randompaper.co> (https://randompaper.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sketch-Chest/chest-registry/issues"
  },
  "homepage": "https://github.com/Sketch-Chest/chest-registry#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "mongoose": "^4.5.7",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "dotenv": "^2.0.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "nyc": "^8.3.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser",
      "node"
    ]
  }
}
