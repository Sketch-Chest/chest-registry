{
  "name": "chest-registry",
  "version": "1.0.0",
  "description": "Chest registry service.",
  "private": true,
  "scripts": {
    "heroku-postinstall": "cd client; npm install; npm run build",
    "start": "node ./bin/www",
    "watch": "run-p watch:*",
    "watch:server": "nodemon --exec babel-node --inspect --ignore client/ --ignore public/ ./bin/www",
    "watch:client": "cd client; npm start",
    "test": "run-p test:*",
    "test:server": "nyc ava",
    "test:client": "cd client; npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Yasuaki Uechi <uetchy@randompaper.co> (https://randompaper.co/)",
  "license": "MIT",
  "homepage": "https://github.com/Sketch-Chest/chest-registry#readme",
  "dependencies": {
    "auth0-js": "^8.11.3",
    "body-parser": "^1.17.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "mongoose": "^4.13.4",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.9.0",
    "should": "^13.1.3"
  }
}
